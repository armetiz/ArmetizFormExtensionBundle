{
    "name": "armetiz/form-extension-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Bundle to extend Form",
    "keywords": ["symfony", "bundle", "form", "entity", "ajax"],
    "homepage": "http://www.armetiz.info",
    "license": "BSD",
    "authors": [
        {
            "name": "Thomas Tourlourat",
            "email": "thomas@tourlourat.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": ">=2.1.0"
    },
    "require-dev": {
        "symfony/stopwatch": ">=2.2,<2.4-dev",
        "symfony/dependency-injection": "~2.0",
        "symfony/form": ">=2.2,<2.4-dev",
        "doctrine/dbal": "~2.2",
        "doctrine/orm": "~2.2,>=2.2.3"
    },
    "autoload": {
        "psr-0": { "Armetiz\\FormExtensionBundle": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "target-dir": "Armetiz/FormExtensionBundle"
}
